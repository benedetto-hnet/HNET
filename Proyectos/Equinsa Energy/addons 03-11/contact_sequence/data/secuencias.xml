<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Sequences for Client Partner -->
        <record id="client_partner_sequence" model="ir.sequence">
            <field name="name">Client Partner Sequence</field>
            <field name="code">client.partner.sequence</field>
            <field name="prefix">CLIE</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for supplier Partner -->
        <record id="supplier_partner_sequence" model="ir.sequence">
            <field name="name">Supplier Partner Sequence</field>
            <field name="code">supplier.partner.sequence</field>
            <field name="prefix">PROV</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
