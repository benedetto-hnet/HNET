<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for Requisitions -->
        <record id="requisicion_seq" model="ir.sequence">
            <field name="name">Requisicion</field>
            <field name="code">requisicion.sequence</field>
            <field name="prefix">RE/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for Requisitions ABS -->
        <record id="requisicion_ra_seq" model="ir.sequence">
            <field name="name">Requisicion</field>
            <field name="code">ra.sequence</field>
            <field name="prefix">RA/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        <!-- Sequences for Requisitions GAS-->
        <record id="requisicion_rg_seq" model="ir.sequence">
            <field name="name">Requisicion</field>
            <field name="code">rg.sequence</field>
            <field name="prefix">RG/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Sequences for Requisitions Condensada -->
        <record id="requisicion_condensada_seq" model="ir.sequence">
            <field name="name">Requisicion Condensada</field>
            <field name="code">requisicion.con.sequence</field>
            <field name="prefix">REC/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
